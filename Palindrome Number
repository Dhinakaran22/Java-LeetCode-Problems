class Solution {
    public boolean isPalindrome(int x) {
        int n =0;
        int y = x;
        while (x > 0) {
            int m = x % 10;
            x = x / 10;
            n = n * 10 + m;
        }
        if (y != n) {
            return false;
        } 
        else {
            return true;
        }
    }
}
